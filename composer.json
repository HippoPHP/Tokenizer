{
    "name": "hippophp/tokenizer",
    "description": "Wrapper around token_get_all",
    "type": "library",
    "keywords": ["hippophp","tokenizer","token"],
    "homepage": "https://github.com/HippoPHP/Tokenizer",
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Marcin Kurczewski",
            "homepage": "http://sakuya.pl"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-tokenizer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "support": {
        "wiki": "https://github.com/hippophp/tokenizer/wiki",
        "source": "https://github.com/hippophp/tokenizer"
    },
    "autoload": {
        "psr-4": {
            "HippoPHP\\Tokenizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HippoPHP\\Tests\\Tokenizer\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
